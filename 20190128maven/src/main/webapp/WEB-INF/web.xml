<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>page</servlet-name>
  	<display-name>page</display-name>
  	<description></description>
  	<servlet-class>com.java.page</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>page2</servlet-name>
  	<display-name>page2</display-name>
  	<description></description>
  	<servlet-class>com.java.page2</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>View</servlet-name>
  	<display-name>View</display-name>
  	<description></description>
  	<servlet-class>com.java.View</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>page5</servlet-name>
  	<display-name>page5</display-name>
  	<description></description>
  	<servlet-class>com.java.page5</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>page</servlet-name>
  	<url-pattern>/page</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>page2</servlet-name>
  	<url-pattern>/page2/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>View</servlet-name>
  	<url-pattern>/View/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>page5</servlet-name>
  	<url-pattern>/page5</url-pattern>
  </servlet-mapping>
</web-app>
